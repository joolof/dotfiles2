let SessionLoad = 1
let s:so_save = &g:so | let s:siso_save = &g:siso | setg so=0 siso=0 | setl so=-1 siso=-1
let v:this_session=expand("<sfile>:p")
silent only
silent tabonly
cd ~/Codes/C/gasdrag
if expand('%') == '' && !&modified && line('$') <= 1 && getline(1) == ''
  let s:wipebuf = bufnr('%')
endif
set shortmess=aoO
badd +669 ~/Codes/C/gasdrag/height.py
badd +198 ~/Codes/C/gasdrag/gasdrag.c
badd +1 ~/Codes/C/gasdrag/gasdrag.h
badd +62 ~/Codes/C/gasdrag/single_grain/check_dt.py
badd +72 ~/Codes/C/gasdrag/single_grain/onegrain.c
badd +1 ~/Codes/C/gasdrag/single_grain/eta.py
badd +75 ~/Codes/C/gasdrag/get_sb.py
badd +5 ~/Codes/C/gasdrag/models/HD32297.csv
badd +5 ~/Codes/C/gasdrag/biblio/bib_plot.py
badd +125 ~/Codes/C/gasdrag/sb_binned.py
argglobal
%argdel
edit ~/Codes/C/gasdrag/height.py
let s:save_splitbelow = &splitbelow
let s:save_splitright = &splitright
set splitbelow splitright
wincmd _ | wincmd |
vsplit
1wincmd h
wincmd w
let &splitbelow = s:save_splitbelow
let &splitright = s:save_splitright
wincmd t
let s:save_winminheight = &winminheight
let s:save_winminwidth = &winminwidth
set winminheight=0
set winheight=1
set winminwidth=0
set winwidth=1
exe 'vert 1resize ' . ((&columns * 103 + 115) / 230)
exe 'vert 2resize ' . ((&columns * 126 + 115) / 230)
argglobal
balt ~/Codes/C/gasdrag/gasdrag.c
setlocal fdm=manual
setlocal fde=0
setlocal fmr={{{,}}}
setlocal fdi=#
setlocal fdl=0
setlocal fml=1
setlocal fdn=20
setlocal fen
silent! normal! zE
let &fdl = &fdl
let s:l = 669 - ((40 * winheight(0) + 24) / 48)
if s:l < 1 | let s:l = 1 | endif
keepjumps exe s:l
normal! zt
keepjumps 669
normal! 09|
wincmd w
argglobal
if bufexists("~/Codes/C/gasdrag/gasdrag.c") | buffer ~/Codes/C/gasdrag/gasdrag.c | else | edit ~/Codes/C/gasdrag/gasdrag.c | endif
if &buftype ==# 'terminal'
  silent file ~/Codes/C/gasdrag/gasdrag.c
endif
balt ~/Codes/C/gasdrag/sb_binned.py
setlocal fdm=manual
setlocal fde=0
setlocal fmr={{{,}}}
setlocal fdi=#
setlocal fdl=0
setlocal fml=1
setlocal fdn=20
setlocal fen
silent! normal! zE
let &fdl = &fdl
let s:l = 579 - ((23 * winheight(0) + 24) / 48)
if s:l < 1 | let s:l = 1 | endif
keepjumps exe s:l
normal! zt
keepjumps 579
normal! 040|
wincmd w
exe 'vert 1resize ' . ((&columns * 103 + 115) / 230)
exe 'vert 2resize ' . ((&columns * 126 + 115) / 230)
tabnext 1
if exists('s:wipebuf') && len(win_findbuf(s:wipebuf)) == 0&& getbufvar(s:wipebuf, '&buftype') isnot# 'terminal'
  silent exe 'bwipe ' . s:wipebuf
endif
unlet! s:wipebuf
set winheight=1 winwidth=20 shortmess=filnxtToOFAI
let &winminheight = s:save_winminheight
let &winminwidth = s:save_winminwidth
let s:sx = expand("<sfile>:p:r")."x.vim"
if filereadable(s:sx)
  exe "source " . fnameescape(s:sx)
endif
let &g:so = s:so_save | let &g:siso = s:siso_save
doautoall SessionLoadPost
unlet SessionLoad
