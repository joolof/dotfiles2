let SessionLoad = 1
let s:so_save = &g:so | let s:siso_save = &g:siso | setg so=0 siso=0 | setl so=-1 siso=-1
let v:this_session=expand("<sfile>:p")
silent only
silent tabonly
cd ~/SPHERE/dpol
if expand('%') == '' && !&modified && line('$') <= 1 && getline(1) == ''
  let s:wipebuf = bufnr('%')
endif
let s:shortmess_save = &shortmess
if &shortmess =~ 'A'
  set shortmess=aoOA
else
  set shortmess=aoO
endif
badd +277 ~/SPHERE/dpol/hexabin.py
badd +24 ~/SPHERE/dpol/get_dust.py
badd +242 ~/SPHERE/dpol/get_dpol.py
badd +59 ~/SPHERE/dpol/obslog.py
badd +178 ~/SPHERE/dpol/get_pdfunc.py
badd +8 ~/SPHERE/dpol/size_dist_dpol.py
argglobal
%argdel
edit ~/SPHERE/dpol/obslog.py
let s:save_splitbelow = &splitbelow
let s:save_splitright = &splitright
set splitbelow splitright
wincmd _ | wincmd |
vsplit
1wincmd h
wincmd w
let &splitbelow = s:save_splitbelow
let &splitright = s:save_splitright
wincmd t
let s:save_winminheight = &winminheight
let s:save_winminwidth = &winminwidth
set winminheight=0
set winheight=1
set winminwidth=0
set winwidth=1
exe 'vert 1resize ' . ((&columns * 153 + 153) / 307)
exe 'vert 2resize ' . ((&columns * 153 + 153) / 307)
argglobal
balt ~/SPHERE/dpol/get_pdfunc.py
setlocal fdm=manual
setlocal fde=0
setlocal fmr={{{,}}}
setlocal fdi=#
setlocal fdl=0
setlocal fml=1
setlocal fdn=20
setlocal fen
silent! normal! zE
let &fdl = &fdl
let s:l = 30 - ((17 * winheight(0) + 29) / 59)
if s:l < 1 | let s:l = 1 | endif
keepjumps exe s:l
normal! zt
keepjumps 30
normal! 0134|
wincmd w
argglobal
if bufexists(fnamemodify("~/SPHERE/dpol/obslog.py", ":p")) | buffer ~/SPHERE/dpol/obslog.py | else | edit ~/SPHERE/dpol/obslog.py | endif
if &buftype ==# 'terminal'
  silent file ~/SPHERE/dpol/obslog.py
endif
balt ~/SPHERE/dpol/get_pdfunc.py
setlocal fdm=manual
setlocal fde=0
setlocal fmr={{{,}}}
setlocal fdi=#
setlocal fdl=0
setlocal fml=1
setlocal fdn=20
setlocal fen
silent! normal! zE
let &fdl = &fdl
let s:l = 38 - ((25 * winheight(0) + 29) / 59)
if s:l < 1 | let s:l = 1 | endif
keepjumps exe s:l
normal! zt
keepjumps 38
normal! 0
wincmd w
2wincmd w
exe 'vert 1resize ' . ((&columns * 153 + 153) / 307)
exe 'vert 2resize ' . ((&columns * 153 + 153) / 307)
tabnext 1
if exists('s:wipebuf') && len(win_findbuf(s:wipebuf)) == 0 && getbufvar(s:wipebuf, '&buftype') isnot# 'terminal'
  silent exe 'bwipe ' . s:wipebuf
endif
unlet! s:wipebuf
set winheight=1 winwidth=20
let &shortmess = s:shortmess_save
let &winminheight = s:save_winminheight
let &winminwidth = s:save_winminwidth
let s:sx = expand("<sfile>:p:r")."x.vim"
if filereadable(s:sx)
  exe "source " . fnameescape(s:sx)
endif
let &g:so = s:so_save | let &g:siso = s:siso_save
set hlsearch
doautoall SessionLoadPost
unlet SessionLoad
" vim: set ft=vim :
