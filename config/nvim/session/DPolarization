let SessionLoad = 1
let s:so_save = &g:so | let s:siso_save = &g:siso | setg so=0 siso=0 | setl so=-1 siso=-1
let v:this_session=expand("<sfile>:p")
silent only
silent tabonly
cd ~/SPHERE/dpol
if expand('%') == '' && !&modified && line('$') <= 1 && getline(1) == ''
  let s:wipebuf = bufnr('%')
endif
let s:shortmess_save = &shortmess
if &shortmess =~ 'A'
  set shortmess=aoOA
else
  set shortmess=aoO
endif
badd +86 ~/SPHERE/dpol/hexabin.py
badd +12 ~/SPHERE/dpol/obslog_v2.py
badd +144 ~/SPHERE/dpol/DPol_Optool.py
badd +43 ~/SPHERE/dpol/DPol_AggscatVir.py
badd +32 ~/SPHERE/dpol/test_optool.py
badd +11 ~/SPHERE/dpol/results_to_latex.py
badd +1 ~/SPHERE/dpol/chains/HD_115600_0_results.txt
badd +13 ~/SPHERE/dpol/DPol_Max_comparison.py
badd +1 ~/SPHERE/dpol/priors/HD_129590.csv
badd +6 ~/SPHERE/dpol/priors/HD_115600.csv
argglobal
%argdel
edit ~/SPHERE/dpol/results_to_latex.py
wincmd t
let s:save_winminheight = &winminheight
let s:save_winminwidth = &winminwidth
set winminheight=0
set winheight=1
set winminwidth=0
set winwidth=1
argglobal
balt ~/SPHERE/dpol/chains/HD_115600_0_results.txt
setlocal fdm=manual
setlocal fde=0
setlocal fmr={{{,}}}
setlocal fdi=#
setlocal fdl=0
setlocal fml=1
setlocal fdn=20
setlocal fen
silent! normal! zE
let &fdl = &fdl
let s:l = 11 - ((10 * winheight(0) + 30) / 60)
if s:l < 1 | let s:l = 1 | endif
keepjumps exe s:l
normal! zt
keepjumps 11
normal! 070|
tabnext 1
if exists('s:wipebuf') && len(win_findbuf(s:wipebuf)) == 0 && getbufvar(s:wipebuf, '&buftype') isnot# 'terminal'
  silent exe 'bwipe ' . s:wipebuf
endif
unlet! s:wipebuf
set winheight=1 winwidth=20
let &shortmess = s:shortmess_save
let &winminheight = s:save_winminheight
let &winminwidth = s:save_winminwidth
let s:sx = expand("<sfile>:p:r")."x.vim"
if filereadable(s:sx)
  exe "source " . fnameescape(s:sx)
endif
let &g:so = s:so_save | let &g:siso = s:siso_save
set hlsearch
doautoall SessionLoadPost
unlet SessionLoad
" vim: set ft=vim :
