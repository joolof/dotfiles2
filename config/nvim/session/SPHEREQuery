let SessionLoad = 1
let s:so_save = &g:so | let s:siso_save = &g:siso | setg so=0 siso=0 | setl so=-1 siso=-1
let v:this_session=expand("<sfile>:p")
silent only
silent tabonly
cd ~/Codes/Python/Projects/SPHEREQuery
if expand('%') == '' && !&modified && line('$') <= 1 && getline(1) == ''
  let s:wipebuf = bufnr('%')
endif
let s:shortmess_save = &shortmess
if &shortmess =~ 'A'
  set shortmess=aoOA
else
  set shortmess=aoO
endif
badd +1 ~/Codes/Python/Projects/SPHEREQuery/dl_window.py
badd +1 ~/Codes/Python/Projects/SPHEREQuery/pref_window.py
badd +22 ~/Codes/Python/Projects/SPHEREQuery/do_query.py
badd +6 ~/Codes/Python/Projects/SPHEREQuery/data_reduction.py
badd +1 ~/Codes/Python/Projects/SPHEREQuery/cm_window.py
badd +1 ~/Codes/Python/Projects/SPHEREQuery/SPHEREQuery.py
badd +41 ~/Codes/Python/Projects/SPHEREQuery/style.qss
badd +38 ~/Codes/Python/Projects/SPHEREQuery/README.md
badd +1 ~/Codes/Python/Projects/SPHEREQuery/requirements.txt
argglobal
%argdel
edit ~/Codes/Python/Projects/SPHEREQuery/SPHEREQuery.py
let s:save_splitbelow = &splitbelow
let s:save_splitright = &splitright
set splitbelow splitright
wincmd _ | wincmd |
vsplit
1wincmd h
wincmd w
let &splitbelow = s:save_splitbelow
let &splitright = s:save_splitright
wincmd t
let s:save_winminheight = &winminheight
let s:save_winminwidth = &winminwidth
set winminheight=0
set winheight=1
set winminwidth=0
set winwidth=1
exe 'vert 1resize ' . ((&columns * 113 + 113) / 227)
exe 'vert 2resize ' . ((&columns * 113 + 113) / 227)
argglobal
balt ~/Codes/Python/Projects/SPHEREQuery/style.qss
setlocal fdm=manual
setlocal fde=0
setlocal fmr={{{,}}}
setlocal fdi=#
setlocal fdl=0
setlocal fml=1
setlocal fdn=20
setlocal fen
silent! normal! zE
let &fdl = &fdl
let s:l = 1 - ((0 * winheight(0) + 24) / 49)
if s:l < 1 | let s:l = 1 | endif
keepjumps exe s:l
normal! zt
keepjumps 1
normal! 0
wincmd w
argglobal
if bufexists(fnamemodify("~/Codes/Python/Projects/SPHEREQuery/style.qss", ":p")) | buffer ~/Codes/Python/Projects/SPHEREQuery/style.qss | else | edit ~/Codes/Python/Projects/SPHEREQuery/style.qss | endif
if &buftype ==# 'terminal'
  silent file ~/Codes/Python/Projects/SPHEREQuery/style.qss
endif
balt ~/Codes/Python/Projects/SPHEREQuery/SPHEREQuery.py
setlocal fdm=manual
setlocal fde=0
setlocal fmr={{{,}}}
setlocal fdi=#
setlocal fdl=0
setlocal fml=1
setlocal fdn=20
setlocal fen
silent! normal! zE
let &fdl = &fdl
let s:l = 41 - ((33 * winheight(0) + 24) / 49)
if s:l < 1 | let s:l = 1 | endif
keepjumps exe s:l
normal! zt
keepjumps 41
normal! 024|
wincmd w
exe 'vert 1resize ' . ((&columns * 113 + 113) / 227)
exe 'vert 2resize ' . ((&columns * 113 + 113) / 227)
tabnext 1
if exists('s:wipebuf') && len(win_findbuf(s:wipebuf)) == 0 && getbufvar(s:wipebuf, '&buftype') isnot# 'terminal'
  silent exe 'bwipe ' . s:wipebuf
endif
unlet! s:wipebuf
set winheight=1 winwidth=20
let &shortmess = s:shortmess_save
let &winminheight = s:save_winminheight
let &winminwidth = s:save_winminwidth
let s:sx = expand("<sfile>:p:r")."x.vim"
if filereadable(s:sx)
  exe "source " . fnameescape(s:sx)
endif
let &g:so = s:so_save | let &g:siso = s:siso_save
set hlsearch
nohlsearch
doautoall SessionLoadPost
unlet SessionLoad
" vim: set ft=vim :
